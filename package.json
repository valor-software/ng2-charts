    "core-js": "^3.19.0",
    "date-fns": "^2.25.0",
    "highlight.js": "^11.3.1",
    "lodash-es": "^4.17.21",
    "ngx-highlightjs": "^4.1.4",
    "ngx-markdown": "^12.0.1",
    "prismjs": "^1.25.0",
    "rxjs": "^6.6.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.1",
    "@angular-devkit/core": "^13.0.1",
