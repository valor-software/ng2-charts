    "e2e:ci": "ng e2e",
    "e2e:update-snapshots": "npm run e2e:ci -- --configuration update-snapshots",
    "cy:verify": "cypress verify",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
