{
  "name": "ng2-charts",
  "version": "1.0.0-beta.0",
  "description": "Reactive, responsive, beautiful charts for Angular2 based on Chart.js",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "prepublish": "gulp clean && tsc",
    "postinstall": "typings install --ambient",
    "server": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "start": "npm run server",
    "test": "gulp lint"
  },
  "main": "ng2-charts.js",
  "typings": "ng2-charts.d.ts",
  "keywords": [
    "angular2",
    "angularjs",
    "charts"
  ],
  "author": "Dmitriy Shekhovtsov <valorkin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-charts.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-charts/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-charts#readme",
  "dependencies": {},
  "devDependencies": {
    "angular2": "2.0.0-beta.14",
    "bootstrap": "3.3.6",
    "chart.js": "1.0.2",
    "clean-webpack-plugin": "0.1.8",
    "compression-webpack-plugin": "0.3.1",
    "es6-shim": "0.35.0",
    "eslint": "^2.7.0",
    "exports-loader": "0.6.3",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-eslint": "^2.0.0",
    "gulp-size": "2.1.0",
    "gulp-tsc": "1.1.4",
    "gulp-tslint": "4.3.5",
    "html-loader": "0.4.2",
    "markdown-loader": "0.1.7",
    "marked": "0.3.5",
    "ng2-bootstrap": "1.0.11",
    "pre-commit": "1.1.2",
    "prismjs": "valorkin/prism",
    "prismjs-loader": "0.0.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "require-dir": "0.3.0",
    "rxjs": "5.0.0-beta.2",
    "transfer-webpack-plugin": "0.1.4",
    "ts-loader": "0.8.1",
    "tslint": "3.5.0",
    "typescript": "1.8.9",
    "typings": "^0.7.12",
