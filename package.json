    "@nrwl/cypress": "^13.1.3",
    "@schematics/angular": "^13.0.1",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash-es": "^4.17.5",
    "@types/luxon": "^1.27.1",
    "@types/node": "^14.17.32",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cypress": "^8.7.0",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^7.32.0",
    "eyes.images": "^3.8.7",
